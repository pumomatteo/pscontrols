{
  "version": "1.0.0",
  "name": "pscontrols",
  "scripts": {
    "compodoc": "npx compodoc src -p tsconfig.json --output ./dist/documentation --disableSourceCode --disablePrivate --disableProtected --disableInternal --disableLifeCycleHooks --disableRoutesGraph --disableDependencies",
    "build:legacy": "vite build && cp dist/vrcontrols.all.min.d.ts dist/vrcontrols.d.ts",
    "build:react": "vite build --mode react",
    "build:esm": "vite build --mode esm",
    "build:combined": "vite build --mode combined",
    "build:all": "npm run build:legacy && npm run build:react && npm run build:esm && npm run build:combined",
    "dev:react": "vite --mode react",
    "dev:legacy": "vite"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.25",
    "@types/jquery": "^3.5.31",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.16",
    "javascript-obfuscator": "^4.1.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.31",
    "prettier": "^3.3.3",
    "terser": "^5.44.0",
    "typescript": "^5.6.2",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "i18next": "^25.5.2",
    "jquery": "^3.7.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
