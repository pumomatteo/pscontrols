_divContainer\").find(\".groupBy\"+t).hide());0==i.groupBy.fields.length&&(i.groupBy=null),t&&this.update()}}removeAllGroups(e=!0){let t=this.getOptions();t.groupable&&null!=t.groupBy&&this.removeGroups(t.groupBy.fields.map(e=>e.field),!1),t.groupBy=null,e&&this.update()}addGroup(e,t=!0,i,a){this.addGroups([e],t,i,a)}addGroups(e,t=!0,i,a){let n=this.getOptions();if(null==n.groupBy&&(n.groupBy=[]),!n.groupable)throw new Error(\"Griglia non raggruppabile! Mettere opzione .groupable: true\");null!=i&&(n.groupBy.sortBy=i),null!=a&&(n.groupBy.internalSortBy=a);for(let t of e){let e=t;if(\"string\"==typeof t&&(e=new vr_1.GridGroupByItem,e.field=t),null==n.groupBy.fields&&(n.groupBy.fields=[]),!n.groupBy.fields.map(e=>e.field).includes(e.field)){n.groupBy.fields.push(e);let i=t;\"string\"!=typeof t&&(i=t.field),vr_1.puma(\"#\"+this._elementId+\"_divContainer\").find(\".groupBy\"+i).show()}}this.sortingGroupFields(this.dataSource()),t&&this.update()}getChildrenGroupRows(e){let t=new GridChildrenGroupRows,i=[],a=Number(vr_1.puma(e).attr(\"level\")),n=vr_1.puma(e).index()+2,o=vr_1.puma(this._divBody).find(\"tr:nth-child(\"+n+\")\"),r=Number(vr_1.puma(this._divBody).find(\"tr:nth-child(\"+n+\")\").attr(\"level\"));for(i.push(o[0]);(0==a&&r!=a||a!=r&&!o.hasClass(\"grid_trGroupBy\"))&&0!=o.length;)i.map(e=>vr_1.puma(e).attr(\"id\")).includes(o.attr(\"id\"))&&!o.hasClass(\"grid_trGroupBy\")||i.push(o[0]),n++,o=vr_1.puma(this._divBody).find(\"tr:nth-child(\"+n+\")\"),r=Number(vr_1.puma(this._divBody).find(\"tr:nth-child(\"+n+\")\").attr(\"level\"));return i=i.filter(e=>!vr_1.puma(e).hasClass(\"p-grid-totalsGroup\")),t.children=i.filter(e=>!vr_1.puma(e).hasClass(\"grid_trGroupBy\")),t.groupRows=i.filter(e=>vr_1.puma(e).hasClass(\"grid_trGroupBy\")),t.allRows=i,t}getCheckedChildrenGroupRows(e){let t=[],i=this.getChildrenGroupRows(e);for(let e of i.children){let i=vr_1.puma(e).find(\".vr-checkbox-column\")[0];null!=i&&i.checked&&t.push(e)}return t}createWindowTableActions(){this.getO