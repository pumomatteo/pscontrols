ato\":d)+\"</div>\"+(null!=options.onGroupEditClick||null!=n.onEditClick?\"<div style='position: relative; display: inline-flex; top: -4px; margin-left: 6px;'><i class='grid_groupByEdit fa fa-pencil' style='cursor: pointer;'></div></i>\":\"\")+\"</td>\";if(vr_1.puma(s).appendPuma(m),this._groupByActualValue[n.field]=r,options.checkboxes!=vr_1.GridCheckboxModeEnum.None){let e=vr_1.puma(s).find(\"input.grid_trChckGroupBy\")[0].id;vr_1.createCheckBox({onCheck:e=>{let t=e.sender.element().parentElement.parentElement.parentElement.parentElement,i=this.getChildrenGroupRows(t);for(let t of i.children){let i=vr_1.puma(t).attr(\"id\");if(null!=i&&i.length>0){let n=i.split(\"_\")[1];if(e.checked){vr_1.puma(t).find(\".vr-checkbox-column\")[0].checked&&this.unselectRow(n)}this.selectRowInternal(n,!0,!0)}}}},null,null,e)}vr_1.puma(s).find(\"td.grid_tdGroupByCollapse\").click(t=>{let i=vr_1.puma(t.currentTarget).find(\"i\"),r=i.hasClass(\"fa-caret-down\"),l=this.getChildrenGroupRows(t.currentTarget.parentElement);for(let e of l.allRows)if(r)vr_1.puma(e).hide();else{vr_1.puma(e).show();let t=vr_1.puma(e).find(\"i.grid_tdGroupByCollapse\");t.removeClass(\"fa-caret-right\"),t.addClass(\"fa-caret-down\")}if(r?(i.removeClass(\"fa-caret-down\"),i.addClass(\"fa-caret-right\")):(i.removeClass(\"fa-caret-right\"),i.addClass(\"fa-caret-down\")),null!=options.onGroupExpandCollapse||null!=n.onExpandCollapse){let t=new vr_1.GridGroupExpandCollapseEvent;t.sender=this,t.groupByField=n.field,t.groupByDisplayField=n.displayField,t.collapse=r,t.value=e[n.field],null!=n.displayField&&(t.displayValue=e[n.displayField]);let i=[];for(let e of l.allRows){let t=vr_1.puma(e).attr(\"dataitemid\"),n=this.dataSource().filter(e=>e[options.dataSourceFieldId]==t)[0];i.push(n)}t.childrenItems=i,t.childrenRows=l.allRows,null!=options.onGroupExpandCollapse?options.onGroupExpandCollapse(t):null!=n.onExpandCollapse&&n.onExpandCollapse(t)}}),vr_1.puma(s).find(\".grid_groupByEdit\").click(t=>{if(null!=options.onGroup